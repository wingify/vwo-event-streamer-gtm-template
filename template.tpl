const DATALAYER_VARIABLE_NAME = "dataLayer";

const copyFromDataLayer = require("copyFromDataLayer");
const copyFromWindow = require("copyFromWindow");
const createQueue = require("createQueue");
const getType = require("getType");

function isEventNameReserved(eventName) {
  const defaultEvents = ["gtm.dom", "gtm.load", "gtm.js"];
  const rawReservedNames = data.reservedEventNames || [];
  const reservedNames = [];
  for (let i = 0; i < rawReservedNames.length; i++) {
    reservedNames.push(rawReservedNames[i].eventName);
  }
  const array = defaultEvents.concat(reservedNames);
  
  for (let i = 0; i < array.length; i++) {
    if (array[i].trim() === eventName.trim()) {
      return true;
    }
  }
  return false;
}

function isPropertyExcluded(property) {
  const array = data.reservedProperties || [];
  for (let i = 0; i < array.length; i++) {
    if (array[i].property.trim() === property.trim()) {
      return true;
    }
  }
  return false;
}

function findObjectByProperty(array, property, value) {
  for (let i = 0; i < array.length; i++) {
    if (array[i] && getType(array[i]) == 'object' && array[i][property] === value) {
      return array[i];
    }
  }
  return null;
}

function getCurrentEventObject() {
  const eventID = copyFromDataLayer("gtm.uniqueEventId");
  const dataLayerVar = copyFromWindow(DATALAYER_VARIABLE_NAME);
  const object = findObjectByProperty(
    dataLayerVar,
    "gtm.uniqueEventId",
    eventID
  );
  
  if (!object || !object.event) { 
    return null;
  }
  
  if (isEventNameReserved(object.event)) {
    return null;
  }

    return object; 
}

function buildVwoPayload(obj) {
  let eventName = "";
  const otherProperties = {};
  const vwoMeta = {
    source: "gtm"
  };
  for (const key in obj) {
    if (key === "gtm.uniqueEventId") {
      continue;
    }

    if (obj.hasOwnProperty(key)) {
      const value = obj[key];

      if (key === "event") {
        if (value.slice(0,4) == 'gtm.') {
          eventName = value;
        } else {
          eventName = "gtm." + value;
        }
        vwoMeta.ogName = value;
      } else {
        if (!isPropertyExcluded(key)){
          otherProperties[key] = value;
        }
      }
    }
  }

  return {
    event: eventName,
    props: otherProperties,
    vwoMeta: vwoMeta
  };
}

const DLObject = getCurrentEventObject();
if (DLObject != null) {
  const VWOPayload = buildVwoPayload(DLObject);
  if (VWOPayload) {
    const vwoPush = createQueue("VWO");
    vwoPush(["event", VWOPayload.event, VWOPayload.props, VWOPayload.vwoMeta]);
  }
}

data.gtmOnSuccess();


